    "import sys\n",
    "for line in sys.stdin:\n",
    "    a, b, c = map(int, line.split())\n",
    "\n",
    "M = max(a+b, a*b)\n",
    "if c == 1:\n",
    "    print (M + c)\n",
    "else:\n",
    "    print (M * c)"
